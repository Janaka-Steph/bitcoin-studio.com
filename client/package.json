{
  "name": "bitcoin-studio-client",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx src/",
    "start": "concurrently --kill-others-on-fail -n client,server \"yarn start:client\" \"yarn start:server\"",
    "start:client": "react-scripts start",
    "start:client-okteto": "HOST=0.0.0.0 PORT=8080 react-scripts start",
    "start:fakeprod": "concurrently -n client,server \"serve -l 3000 build\" \"yarn start:server\"",
    "start:server": "cd ../api && yarn dev",
    "start:devwatch": "cra-build-watch -p /function/bitcoin-studio-client/ --react-scripts-version 3.4.0",
    "start:watch": "npm-watch",
    "test": "CI=true react-scripts test"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sweetalert/with-react": "^0.1.1",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.4",
    "@types/sweetalert": "^2.0.4",
    "@types/yup": "^0.26.33",
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "formik": "^2.1.4",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "loglevel": "^1.6.7",
    "mime-types": "^2.1.26",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-focus-lock": "^2.2.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-transition-group": "^4.3.0",
    "source-map-explorer": "^2.4.2",
    "svg-loaders-react": "^2.2.1",
    "sweetalert": "^2.1.2",
    "typescript": "~3.8.3",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cra-build-watch": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-app": "^6.2.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "npm-watch": "^0.6.0"
  }
}
