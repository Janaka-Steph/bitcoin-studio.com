{
  "name": "bitcoin-studio",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "fuse-box-react-scripts": "^3.0.80",
    "fuse-box-react-scripts-ts": "^3.0.80"
  },
  "dependencies": {
    "express": "^4.16.3",
    "fuse-react": "^1.0.21",
    "normalize.css": "^8.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "scripts": {
    "start": "fuse-box-react-scripts start",
    "build": "fuse-box-react-scripts build",
    "test": "fuse-box-react-scripts test --env=jsdom",
